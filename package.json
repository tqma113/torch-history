{
  "name": "torch-history",
  "version": "0.0.0",
  "description": "torch-history is a JavaScript library that lets you easily manage session history anywhere JavaScript runs. history abstracts away the differences in various environments and provides a minimal API that lets you manage the history stack, navigate, confirm navigation, and persist state between sessions.",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent",
    "format": "prettier --write --parser typescript \"{src,__tests__}/**/*.ts?(x)\"",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tqma113/torch-history.git"
  },
  "keywords": [
    "history"
  ],
