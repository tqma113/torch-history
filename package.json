  "bugs": {
    "url": "https://github.com/tqma113/torch-history/issues"
  },
  "homepage": "https://github.com/tqma113/torch-history#readme",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "chalk": "^4.1.0",
    "conventional-changelog": "^3.1.23",
    "conventional-changelog-cli": "^2.1.0",
    "enquirer": "^2.3.6",
    "execa": "^4.0.3",
    "jest": "^26.4.2",
    "minimist": "^1.2.5",
    "prettier": "^2.1.1",
    "semver": "^7.3.2",
    "ts-jest": "^26.3.0",
